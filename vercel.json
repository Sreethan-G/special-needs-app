{
  "version": 2,
  "builds": [
    {
      "src": "special-needs/**/*",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "cd special-needs && expo export --platform web",
        "distDir": "special-needs/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/special-needs/dist/$1"
    }
  ]
}
