{
  "version": 2,
  "builds": [
    {
      "src": "special-needs/**/*",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "cd special-needs && expo build:web",
        "distDir": "special-needs/web-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/special-needs/web-build/$1"
    }
  ]
}
