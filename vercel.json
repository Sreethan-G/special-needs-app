{
    "builds": [
      {
        "src": "special-needs/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "special-needs/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "special-needs/$1"
      }
    ]
  }
  